[MESSAGES CONTROL]
disable=
    missing-docstring,
    consider-using-f-string,
    invalid-name,
    no-self-use,
    too-few-public-methods,
    bad-continuation,
    super-with-arguments

[TYPECHECK]
ignored-classes=scoped_session
extension-pkg-whitelist=lxml,apache_beam
ignored-modules=lxml.builder,apache_beam

[SIMILARITIES]
min-similarity-lines=10
ignore-imports=yes
